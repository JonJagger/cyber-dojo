
$("#traffic-lights")
  .html("<%= js_partial('traffic_lights') %>");

$("#output")
  .html("<%= js_partial('output') %>");

<% @files_to_remove.each do |filename| %>
  to_remove = document.getElementById("<%= filename %>_div");
  if (to_remove) {
    to_remove.parentNode.removeChild(to_remove);
  }
<% end %>

$("#visible-files-container")
  .append("<%= js_partial('new_files') %>");
